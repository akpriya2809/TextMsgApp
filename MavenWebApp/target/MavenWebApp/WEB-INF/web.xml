<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SimpleServlet</servlet-name>
  	<display-name>SimpleServlet</display-name>
  	<description></description>
  	<servlet-class>com.gap.SimpleServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SimpleDataServlet</servlet-name>
  	<display-name>SimpleDataServlet</display-name>
  	<description></description>
  	<servlet-class>com.gap.SimpleDataServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SendMessages</servlet-name>
  	<display-name>SendMessages</display-name>
  	<description></description>
  	<servlet-class>com.gap.SendMessages</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerListServlet</servlet-name>
  	<display-name>CustomerListServlet</display-name>
  	<description></description>
  	<servlet-class>com.gap.CustomerListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SimpleServlet</servlet-name>
  	<url-pattern>/welcom</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SimpleDataServlet</servlet-name>
  	<url-pattern>/SimpleDataServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SendMessages</servlet-name>
  	<url-pattern>/SendMessages</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerListServlet</servlet-name>
  	<url-pattern>/CustomerListServlet</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>
<welcome-file>index.html</welcome-file>
<welcome-file>index.htm</welcome-file>
<welcome-file>index.jsp</welcome-file>
<welcome-file>default.html</welcome-file>
<welcome-file>default.htm</welcome-file>
<welcome-file>default.jsp</welcome-file>
</welcome-file-list>
</web-app>
